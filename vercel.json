{
    "builds": [{
        "src": "StudyRoom/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
        },
        {
            "src": "StudyRoom/asgi.py",
            "use": "@vercel/python",
            "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
            }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "StudyRoom/wsgi.py"
        }
    ]
}